{
	"name" : "kyubi/app",
	"description" : "Minimal scaffold for Kyubi app",
	"type" : "project",
	"keywords" : [
		"Kyubi",
		"PHP",
		"Framework"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Rafael J Torres",
			"email" : "rafaelt88@gmail.com",
			"role" : "Creator & developer"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"repositories" : [{
			"type" : "path",
			"url" : "/var/www/kyubi/*"
		}
	],
	"require" : {
		"kyubi/route" : "@dev"
	},
	"scripts" : {
		"post-install-cmd" : "php -r 'unlink(\"kyubi\");symlink(\"./vendor/bin/kyubi\",\"kyubi\");'"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}
